{
  "name": "solana-trading-bot",
  "version": "1.0.0",
  "description": "Automated trading bot for the Solana blockchain ecosystem",
  "main": "start.js",
  "type": "module",
  "scripts": {
    "start": "node start.js",
    "dev": "nodemon start.js",
    "sim": "node start.js --sim 30",
    "optimize": "node start.js --optimize",
    "debug": "node debug.js",
    "lint": "eslint .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "solana",
    "crypto",
    "trading",
    "bot",
    "defi",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.75.0",
    "@tensorflow/tfjs-node": "^4.0.0",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "date-fns": "^2.29.3",
    "dompurify": "^3.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^6.1.5",
    "jsdom": "^22.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.1",
    "socket.io": "^4.6.1",
    "winston": "^3.8.2",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "extensionsToTreatAsEsm": [".js"],
    "verbose": true,
    "testTimeout": 30000
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/solana-trading-bot.git"
  },
  "bugs": {
    "url": "https://github.com/username/solana-trading-bot/issues"
  },
  "homepage": "https://github.com/username/solana-trading-bot#readme"
