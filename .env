# Server Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# Security
JWT_SECRET=solana_trading_bot_secret_key_change_this_in_production
JWT_EXPIRATION=24h
REFRESH_TOKEN_EXPIRATION=7d

# API Keys - Replace with your actual keys
COINGECKO_API_KEY=demo_api_key

# Solana RPC - You can use public endpoints or your own
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_WS_URL=wss://api.mainnet-beta.solana.com

# Notifications - Replace with your own or leave commented out to disable
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token
# TELEGRAM_CHAT_ID=your_telegram_chat_id
# DISCORD_WEBHOOK_URL=your_discord_webhook_url

# Email notification settings - uncomment and configure to enable
# EMAIL_FROM=bot@solanatrader.com
# EMAIL_TO=your_email@example.com
# EMAIL_HOST=smtp.example.com
# EMAIL_PORT=587
# EMAIL_USER=your_smtp_username
# EMAIL_PASS=your_smtp_password
# EMAIL_SECURE=false

# Audit Logging - Generate strong random keys for production
AUDIT_SIGNATURE_KEY=sample_audit_signature_key_for_development
AUDIT_ENCRYPTION_KEY=sample_audit_encryption_key_for_development

# ML Models
TF_FORCE_GPU_ALLOW_GROWTH=true

# Trading Settings
INITIAL_CAPITAL=10000
MAX_OPEN_POSITIONS=3
TRADE_SIZE_PERCENT=2
STOP_LOSS_PERCENT=5
TAKE_PROFIT_PERCENT=15